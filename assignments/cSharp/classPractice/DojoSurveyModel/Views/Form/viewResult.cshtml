@{
    ViewData["Title"] = "Results";
}

@model Form
<h1>Submitted Info</h1>
<div id="results">
    <div>
        <h2>Name:</h2>
        <h2>@Model.Name</h2>
    </div>
    <div>
        <h3>Dojo Location:</h3>
        <h3>@Model.Location</h3>
    </div>
    <div>
        <h3>Favorite Language:</h3>
        <h3>@Model.Language</h3>
    </div>
    <div>
        <h3>Comment:</h3>
        <h3>@Model.Comment</h3>
    </div>
    <button><a href="/">Go Back</a></button>
</div>